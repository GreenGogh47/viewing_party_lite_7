<% if session[:user_id] %>
  <%= link_to "Log Out", logout_path %><br>

  <h3>Existing Users</h3>

  <% @users.each do |user| %>
    <div id ="user_<%= user.id %>">
      <%= "#{user.name} - #{user.email}" %>
    </div>
  <% end %>

<% else %>
  <%= link_to "Log In", login_path %><br>
  <%= link_to "Create New User", register_path %><br>
<% end %>