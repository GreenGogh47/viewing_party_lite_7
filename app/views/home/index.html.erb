<% if session[:user_id] %>
  <%= link_to "Log Out", logout_path %><br>
<% else %>
  <%= link_to "Log In", login_path %><br>
  <%= link_to "Register", register_path %><br>
<% end %>

<h3>Existing Users</h3>

<% @users.each do |user| %>
  <div id ="user_<%= user.id %>">
    <%= link_to "#{user.name} - #{user.email}", user_path(user.id) %>
  </div>
<% end %>